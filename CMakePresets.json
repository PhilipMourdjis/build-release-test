{
  "version": 3,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 22,
    "patch": 2
  },
  "configurePresets": [
    {
      "name": "default",
      "cacheVariables": {
        "WARNINGS_AS_ERRORS": "ON",
        "PRESET_IN_USE": "ON",
        "CMAKE_BUILD_TYPE": "Release",
        "CMAKE_EXPORT_COMPILE_COMMANDS": "ON",
        "CMAKE_POLICY_DEFAULT_CMP0091": "NEW"
      },
      "warnings": {
        "dev": true
      },
      "errors": {
        "dev": true
      },
      "toolchainFile": "conan_toolchain.cmake"
    }
  ]
}