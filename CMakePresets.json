{
    "version": 3,
    "cmakeMinimumRequired": {
      "major": 3,
      "minor": 20,
      "patch": 0
    },
    "configurePresets": [
      {
        "name": "default",
        "cacheVariables": {
          "WARNINGS_AS_ERRORS": "ON",
          "PRESET_IN_USE": "ON",
          "CMAKE_BUILD_TYPE": "RelWithDebInfo",
          "CMAKE_EXPORT_COMPILE_COMMANDS": "ON"
        },
        "warnings": {
          "dev": true
        },
        "errors": {
          "dev": true
        }
      }
    ]
}